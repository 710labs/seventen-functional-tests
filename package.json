{
	"name": "seventen-functional-tests",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"test:local": "npx playwright test --config=configs/local.config.ts --headed --project=chromium --workers=1",
		"ci:test:local": "npx playwright test --config=configs/local.config.ts --workers=8",
		"ci:test:dev:ca": "playwright test --config=configs/dev.config.ts --workers=10 --grep @CA",
		"ci:test:staging:ca": "npx playwright test --config=configs/staging.config.ts --workers=10 --grep @CA",
		"ci:test:prod:ca": "npx playwright test --config=configs/prod.config.ts --workers=10 --grep @CA ",
		"smoke:test:prod:ca": "npx playwright test smoke-test-ca --config=configs/prod.config.ts --workers=1",
		"smoke:test:prod:fl": "npx playwright test smoke-test-fl --config=configs/prod.config.ts --workers=1",
		"ci:test:dev:fl": "npx playwright test --config=configs/dev.config.ts --workers=10 --grep @FL",
		"ci:test:staging:fl": "npx playwright test --config=configs/staging.config.ts --workers=10 --grep @FL",
		"ci:test:prod:fl": "npx playwright test --config=configs/prod.config.ts --workers=10 --grep @FL",
		"helper:acuityslots:dev": "npx playwright test -g \"Acuity Helpers\" --config=configs/dev.config.ts --project=chromium --workers=10",
		"helper:acuityslots": "npx playwright test -g \"Acuity Automation\" --config=configs/dev.config.ts --project=chromium --workers=10"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/710labs/seventen-functional-tests.git"
	},
	"prettier": {
		"semi": false,
		"singleQuote": true,
		"trailingComma": "all",
		"printWidth": 100,
		"useTabs": true,
		"tabWidth": 2,
		"bracketSpacing": true,
		"jsxBracketSameLine": true,
		"arrowParens": "avoid"
	},
	"keywords": [
		"Playwright",
		"Function Tests"
	],
	"author": "LaDell Erby",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/710labs/seventen-functional-tests/issues"
	},
	"homepage": "https://github.com/710labs/seventen-functional-tests#readme",
	"devDependencies": {
		"@faker-js/faker": "^6.0.0-alpha.5",
		"@playwright/test": "^1.19.0",
		"allure-commandline": "^2.17.2",
		"allure-playwright": "^2.0.0-beta.15",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.0",
		"find-config": "^1.0.0",
		"node-fetch": "^3.2.0",
		"playwright": "^1.19.0",
		"uuid": "^8.3.2"
	},
	"dependencies": {
		"bankers-rounding": "^0.1.3",
		"convert-csv-to-json": "^1.3.3",
		"csvtojson": "^2.0.10"
	}
}
