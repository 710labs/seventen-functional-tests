{
	"name": "seventen-functional-tests",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"test:local:ca": "playwright test --config=configs/local.config.ts --workers=20 --grep @CA",
		"test:local:fl": "playwright test --config=configs/local.config.ts --workers=20 --grep @FL",
		"test:local:mi": "playwright test --config=configs/local.config.ts --workers=20 --grep @MI",
		"ci:test:dev:ca": "playwright test --config=configs/dev.config.ts --workers=10 --grep @CA",
		"ci:test:dev:mi": "playwright test --config=configs/dev.config.ts --workers=10 --grep @MI",
		"ci:test:dev:fl": "npx playwright test --config=configs/dev.config.ts --workers=10 --grep @FL",
		"ci:test:dev:co": "npx playwright test --config=configs/dev.config.ts --workers=10 --grep @CO",
		"ci:test:staging:ca": "npx playwright test --config=configs/staging.config.ts --workers=10 --grep @CA",
		"ci:test:staging:fl": "npx playwright test --config=configs/staging.config.ts --workers=10 --grep @FL",
		"ci:test:staging:mi": "npx playwright test --config=configs/staging.config.ts --workers=10 --grep @MI",
		"ci:test:staging:co": "npx playwright test --config=configs/staging.config.ts --workers=10 --grep @CO",
		"ci:test:prod:ca": "npx playwright test --config=configs/prod-ca.config.ts --workers=10 --grep @CA ",
		"ci:test:prod:fl": "npx playwright test --config=configs/prod-fl.config.ts --workers=10 --grep @FL",
		"ci:test:always-on-dev": "npx playwright test tests/always-on-tests/live.spec.ts --workers=10",
		"smoke:test:prod:ca": "npx playwright test smoke-test-ca --config=configs/prod-ca.config.ts --workers=1",
		"smoke:test:prod:co": "npx playwright test smoke-test-co --config=configs/prod-co.config.ts --workers=1",
		"smoke:test:prod:fl": "npx playwright test smoke-test-fl --config=configs/prod-fl.config.ts --workers=1",
		"smoke:test:prod:mi": "npx playwright test smoke-test-mi --config=configs/prod-mi.config.ts --workers=1",
		"helper:acuityslots:dev": "npx playwright test -g \"Acuity Helpers\" --config=configs/automation.config.ts --project=Desktop Chrome --workers=10",
		"helper:acuityslots": "npx playwright test -g \"Acuity Automation\" --config=configs/automation.config.ts --project=Desktop Chrome --workers=10",
		"helper:possync_orders": "npx playwright test -g \"POS Order Generator\" --config=configs/pos-sync.config.ts --workers=10",
		"load:ca:dev": "npx artillery run load/ca-load-test.yml"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/710labs/seventen-functional-tests.git"
	},
	"prettier": {
		"semi": false,
		"singleQuote": true,
		"trailingComma": "all",
		"printWidth": 100,
		"useTabs": true,
		"tabWidth": 2,
		"bracketSpacing": true,
		"jsxBracketSameLine": true,
		"arrowParens": "avoid"
	},
	"keywords": [
		"Playwright",
		"Function Tests"
	],
	"author": "LaDell Erby",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/710labs/seventen-functional-tests/issues"
	},
	"homepage": "https://github.com/710labs/seventen-functional-tests#readme",
	"devDependencies": {
		"@aws-sdk/client-s3": "^3.267.0",
		"@faker-js/faker": "^6.0.0-alpha.5",
		"@playwright/test": "^1.46.0",
		"@slack/web-api": "^6.8.1",
		"artillery": "^2.0.19",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.0",
		"find-config": "^1.0.0",
		"fs": "^0.0.1-security",
		"node-fetch": "^3.2.0",
		"path": "^0.12.7",
		"playwright-slack-report": "^1.1.25",
		"uuid": "^8.3.2"
	},
	"dependencies": {
		"@slack/types": "^2.8.0",
		"artillery-plugin-fake-data": "^1.1.0",
		"artillery-plugin-faker": "^5.0.2",
		"bankers-rounding": "^0.1.3",
		"convert-csv-to-json": "^1.3.3",
		"csvtojson": "^2.0.10",
		"json2csv": "^5.0.7",
		"playwright-tesults-reporter": "^1.1.0",
		"s3-lambo": "^1.1.1"
	}
}
